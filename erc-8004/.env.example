# ERC-8004 Registration Configuration
# Copy to .env and fill in values

# Ethereum wallet private key (for signing registration tx)
# Use a wallet you control - this becomes the agent owner
PRIVATE_KEY=

# RPC endpoint (public endpoints or Infura/Alchemy)
# Mainnet: https://ethereum-rpc.publicnode.com
# Sepolia: https://ethereum-sepolia-rpc.publicnode.com
RPC_URL=https://ethereum-rpc.publicnode.com

# Network: "sepolia" for testnet, "mainnet" for production
NETWORK=mainnet

# Hippius S3 credentials for pinning registration file
HIPPIUS_S3_ACCESS_KEY=hip_your_access_key_here
HIPPIUS_S3_SECRET_KEY=your_secret_key_here
HIPPIUS_S3_BUCKET=synapz-state

# URI of the registration file (set after running pin-registration)
AGENT_URI=

# Agent ID (set after registration)
AGENT_ID=
